<p id="notice"><%= notice %></p>
<% if @friend != nil %>
  <div class="jumbotron">
    <h1 class="display-4"><%= @friend.first_name %> <%= @friend.last_name %> : <%= @friend.id %></h1>
    <p class="lead"><%= @friend.dob %> <br /> <%= @friend.email %> <br /> <%= @friend.phone %></p>
    <%= link_to 'Back', friends_path , class:"btn btn-dark"%>
<% else %>
  <h1> No friends exists with that name</h1>
<% end %>

</div>